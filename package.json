{
    "name": "zhijiasoft",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite",
        "build": "npm run lint:fix && npm run tsc && vite build",
        "build:test": "npm run lint:fix && npm run tsc && vite build --mode test",
        "tsc": "vue-tsc --noEmit",
        "serve": "vite preview",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix --ext .ts,.tsx,.vue",
        "prepare": "husky install"
    },
    "dependencies": {
        "@element-plus/icons": "^0.0.11",
        "@vitejs/plugin-legacy": "^2.0.0",
        "@vueuse/core": "^9.1.0",
        "element-plus": "^2.0.4",
        "eslint-plugin-vue3-jsx": "^0.0.3-beta.3",
        "pinia": "^2.0.4",
        "rd-utils": "git+https://gitlab.zhijiasoft.com/zhangyunshan/zhijia_utils.git",
        "resize-observer": "^1.0.4",
        "vite-plugin-fz": "^1.0.2",
        "vue": "^3.2.16",
        "vue-router": "^4.0.12",
        "wisdom-plus": "git+https://gitlab.zhijiasoft.com/vue3-base-framework/wisdom-plus-release.git",
        "wp-preprocessor": "^1.0.2",
        "wp-request": "^1.2.1",
        "wp-validate": "^0.0.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^15.0.0",
        "@commitlint/config-conventional": "^15.0.0",
        "@originjs/vite-plugin-commonjs": "^1.0.1",
        "@types/node": "^16.11.11",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "@vitejs/plugin-vue": "^3.0.1",
        "@vitejs/plugin-vue-jsx": "^2.0.0",
        "eslint": "^8.3.0",
        "eslint-plugin-vue": "^8.1.1",
        "husky": "^7.0.4",
        "less": "^4.1.2",
        "lint-staged": "^12.1.2",
        "path": "^0.12.7",
        "sass": "^1.49.9",
        "terser": "^5.14.2",
        "typescript": "^4.7.4",
        "unplugin-auto-import": "^0.11.1",
        "unplugin-auto-import-element-plus-preset": "^1.0.4",
        "unplugin-vue-components": "^0.22.4",
        "vite": "^3.0.2",
        "vite-plugin-vue-setup-extend-plus": "^0.0.2",
        "vue-tsc": "^0.35.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -x @commitlint/config-conventional -E HUSKY_GIT_PARAMS"
        }
    },
    "license": "MIT"
}
