{
    "name": "wisdom-plus-basic",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "eslint src --fix && vite build",
        "tsc": "vue-tsc --noEmit",
        "serve": "vite preview",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix --ext .ts,.tsx,.vue",
        "prepare": "husky install"
    },
    "dependencies": {
        "@element-plus/icons": "^0.0.11",
        "@vueuse/core": "^7.3.0",
        "element-plus": "^1.2.0-beta.3",
        "eslint-plugin-vue3-jsx": "^0.0.3-beta.3",
        "obsession-ui": "^1.0.11",
        "pinia": "^2.0.4",
        "vue": "^3.2.16",
        "vue-router": "^4.0.12",
        "wp-request": "git+https://gitlab.zhijiasoft.com/vue3-base-framework/wp-request.git"
    },
    "devDependencies": {
        "@commitlint/cli": "^15.0.0",
        "@commitlint/config-conventional": "^15.0.0",
        "@types/node": "^16.11.11",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "@vitejs/plugin-vue": "^1.9.3",
        "@vitejs/plugin-vue-jsx": "^1.3.0",
        "eslint": "^8.3.0",
        "eslint-plugin-vue": "^8.1.1",
        "husky": "^7.0.4",
        "less": "^4.1.2",
        "less-loader": "^10.2.0",
        "lint-staged": "^12.1.2",
        "path": "^0.12.7",
        "typescript": "^4.4.3",
        "vite": "^2.6.4",
        "vue-tsc": "^0.3.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -x @commitlint/config-conventional -E HUSKY_GIT_PARAMS"
        }
    }
}
